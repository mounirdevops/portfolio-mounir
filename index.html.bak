		<script> // Configuration du smooth scrolling en js
	$(document).ready(function(){
		$("a.smooth").on('click', function(event) {
			if (this.hash !== "") { // Début if
				event.preventDefault();
				var hash = this.hash;
				$('html, body').animate({
					scrollTop: $(hash).offset().top
				}, 600, function(){
					window.location.hash = hash;
				});
			} // Fin if
		});
	});
